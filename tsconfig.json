{
  "exclude": ["backup/","web-build/","note.txt","test.json"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "typeRoots": [
      "src/@types",
      "node_modules/@types"
    ]
  }
}
